
# Online Python - IDE, Editor, Compiler, Interpreter

import random
 
temp_sensor_value=random.randint(16,40)
 
 
humidity_value=random.randint(30,100)
 
 
if temp_sensor_value>20 or humidity_value>90:
 
 
    print("Temperature value:",temp_sensor_value)
 
 
    print("Humidity value:",humidity_value)
 
 
    print("Hazard detected")
    
